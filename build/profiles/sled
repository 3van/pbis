# Basic build settings

BUILD_VENDOR="likewise"
BUILD_PREFIX="/opt/${BUILD_VENDOR}"
BUILD_SYSCONFDIR="/etc/${BUILD_VENDOR}"
BUILD_LOCALSTATEDIR="/var"

#define profile specific build options
GPAGENT_CPPFLAGS="-DENABLE_SLED_ONLY"

export GPAGENT_CPPFLAGS

# Build product set

BUILD_PRODUCTS="lwise djgtk"
BUILD_SRCDIST="${BUILD_PRODUCTS}"
