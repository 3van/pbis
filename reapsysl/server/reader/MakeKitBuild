make()
{
    API_SOURCES="\
	reader.c    \
	globals.c"

    mk_group \
        GROUP=reader \
        SOURCES="$API_SOURCES" \
        INCLUDEDIRS=". ../include ../../include" \
        HEADERDEPS="reg/lwreg.h reg/regutil.h lwmsg/lwmsg.h lwadvapi.h" \
        LIBDEPS="eventlog lsaclient regclient rsutils lwmsg lwadvapi ldap_r $LIB_PTHREAD $LIB_DL"
}
