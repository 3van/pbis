make()
{
    LOGGING_R_SOURCES="\
	globals.c \
	logger_r.c"

    mk_library \
        LIB=rsyslogging_r \
        SOURCES="$LOGGING_R_SOURCES" \
        INCLUDEDIRS=". ../../include" \
        HEADERDEPS="lwmsg/lwmsg.h lwadvapi.h reg/lwreg.h" \
        LIBDEPS="lwbase lwbase_nothr lwmsg lwmsg_nothr $LIB_PTHREAD"
}
